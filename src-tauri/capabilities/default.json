{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-load",
    "log:default",
    "core:path:default",
    "sql:allow-close",
    "core:app:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "updater:default"
  ]
}
